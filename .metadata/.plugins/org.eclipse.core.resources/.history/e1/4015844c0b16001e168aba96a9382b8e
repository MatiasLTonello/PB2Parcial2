package ar.edu.unlam;

public class Casamiento extends Evento {
    private Usuario agasajada;

    public Casamiento(String nombre, Usuario agasajado, Usuario agasajada) {
        super(nombre, agasajado);
        this.agasajada = agasajada;
    }

    public Usuario getAgasajada() {
        return agasajada;
    }
}